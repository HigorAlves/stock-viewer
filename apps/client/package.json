{
  "name": "@jetpack/client",
  "version": "0.6.3",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "start": "next start",
    "test": "jest -o --silent",
    "test:watch": "jest -o --watch --silent",
    "test:coverage": "jest -u --ci --silent --coverage",
    "test:ci": "jest -u --ci --silent --verbose --passWithNoTests",
    "build": "next build",
    "serve:coverage": "open http://localhost:5000/ && serve coverage/lcov-report",
    "prettier": "npx prettier --write src/**/*.{ts,tsx,json}",
    "lint": "tsc --noEmit && eslint --fix --ext ts,tsx src",
    "storybook": "start-storybook -s ./public -p 6006",
    "build-storybook": "build-storybook -s ./public",
    "lighthouse": "lighthouse http://localhost:3000 --chrome-flags='--no-sandbox --headless --disable-gpu' --output html --output-path ./lighthouse-report.html --view"
  },
  "dependencies": {
    "axios": "^0.21.0",
    "flexboxgrid": "^6.3.1",
    "formik": "^2.2.6",
    "globby": "^11.0.1",
    "lottie-web": "^5.7.6",
    "next": "10.0.3",
    "next-transpile-modules": "^6.0.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-lottie": "^1.2.3",
    "react-redux": "^7.2.2",
    "recharts": "^2.0.3",
    "styled-components": "^5.2.1",
    "@jetpack/store": "*"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@storybook/addon-essentials": "^6.1.11",
    "@storybook/react": "^6.1.11",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.2",
    "@types/jest": "^26.0.19",
    "@types/node": "^14.14.13",
    "@types/react": "^17.0.0",
    "@types/react-redux": "^7.1.15",
    "@types/redux-saga": "^0.10.5",
    "babel-plugin-inline-react-svg": "^1.1.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "lighthouse": "^7.2.0",
    "next-compose-plugins": "^2.2.1",
    "npm-run-all": "^4.1.5",
    "serve": "^11.3.2",
    "standard-version": "^9.0.0",
    "typescript": "^4.1.3",
    "webp-loader": "^0.6.0"
  }
}
